{
  "name": "share-wheels-backend",
  "version": "0.0.0",
  "private": true,
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "worker": "node workers/notificationWorker.js",
    "start:dev": "cross-env NODE_ENV=development nodemon app.js",
    "start:prod": "cross-env NODE_ENV=production node app.js",
    "docker:dev": "docker compose --env-file .env.dev up --build",
    "docker:prod": "docker compose --env-file .env.prod up --build -d",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "bullmq": "^5.65.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "dotenv": "^16.5.0",
    "express": "~4.16.1",
    "express-rate-limit": "^8.2.1",
    "firebase-admin": "^13.4.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "pg-promise": "^11.14.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.10",
    "prettier": "3.6.2"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules/",
      "*.log"
    ],
    "delay": "2500"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": "prettier --write"
  }
}
